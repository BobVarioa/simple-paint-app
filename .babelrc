{
	"presets": [
		[
			"@babel/env",
			{
				"targets": {
					"browsers": "last 2 Chrome versions",
					"node": "current"
				}
			}
		]
	],
	"plugins": [
		"@babel/proposal-class-properties",
		["@babel/plugin-proposal-decorators", { "decoratorsBeforeExport": true }],
		"@babel/plugin-syntax-jsx"
	]
}
